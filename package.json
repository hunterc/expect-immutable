{
  "name": "expect-immutable",
  "version": "0.0.2",
  "description": "immutable js extension for expect.js",
  "main": "index-dist.js",
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "build": "babel index.js -o index-dist.js",
    "build:watch": "npm run build -- -w",
    "lint": "eslint index.js index-test.js",
    "test": "mocha --compilers js:babel-core/register index-test.js",
    "test:watch": "npm test -- --watch"
  },
  "author": "Hunter Cassidy <hwcassidy@gmail.com>",
  "license": "MIT",
  "keywords": [
    "expect",
    "expect-extension"
  ],
  "peerDependencies": {
    "expect": "^1.0.0",
    "immutable": "^3.7.5"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "expect": "^1.13.4",
    "immutable": "^3.7.6"
  },
  "dependencies": {
    "expect": "^1.13.4",
    "immutable": "^3.7.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunterc/expect-immutable.git"
  },
  "bugs": {
    "url": "https://github.com/hunterc/expect-immutable/issues"
  },
  "homepage": "https://github.com/hunterc/expect-immutable#readme"
}
